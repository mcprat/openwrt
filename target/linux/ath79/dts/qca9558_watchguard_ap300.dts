#include "qca955x_senao_ap.dtsi"

/ {
	compatible = "watchguard,ap300", "qca,qca9558";
	model = "WatchGuard AP300";

	leds {
		compatible = "gpio-leds";

		led_power: power {
			label = "green:power";
			gpios = <&gpio 22 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		lan_link {
			label = "amber:lan";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		lan_data {
			label = "green:lan";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		wifi_amber {
			label = "amber:wifi";
			gpios = <&gpio 23 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		wifi_green {
			label = "green:wifi";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};

	watchdog {
		compatible = "linux,wdt-gpio";
		gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		hw_margin_ms = <20000>;
		always-running;
	};

	virtual_flash {
		devices = <&fwconcat0 &fwconcat1 &fwconcat2>;
	};
};

&partitions {
	fwconcat2: partition@ff0000 {
		label = "fwconcat2";
		reg = <0xff0000 0x1000000>;
	};

	art: partition@1ff0000 {
		label = "art";
		reg = <0x1ff0000 0x010000>;
		read-only;
	};
};

&wdt {
	status = "disabled";
};

&mdio0 {
	status = "okay";

	phy5: ethernet-phy@5 {
		reg = <5>;
		eee-broken-100tx;
		eee-broken-1000t;
	};
};

&eth0 {
	phy-handle = <&phy5>;
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&art 0x0>;
	mtd-mac-address-increment = <1>;
};
