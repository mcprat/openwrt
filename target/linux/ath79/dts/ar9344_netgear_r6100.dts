#include "ar9344.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "netgear,r6100", "qca,ar9344";
	model = "Netgear R6100";

	aliases {
		label-mac-device = &eth0;
		led-boot = &led_power_amber;
		led-failsafe = &led_power_amber;
		led-running = &led_power_green;
		led-upgrade = &led_power_amber;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wifi {
			label = "rfkill";
			linux,code = <KEY_RFKILL>;
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wps {
			label = "WPS";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power_amber: power_amber {
			label = "amber:power";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		led_power_green: power_green {
			label = "green:power";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		usb {
			label = "blue:usb";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "usbport";
			trigger-sources = <&hub_port1>;
		};

		wlan {
			label = "blue:wlan";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		wan_amber {
			label = "amber:wan";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		wan_green {
			label = "green:wan";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-export {
		compatible = "gpio-export";

		usb_power {
			gpio-export,name = "power:usb";
			gpio-export,output = <1>;
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
		};
	};
};

&nand {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x20000>;
			read-only;
		};

		caldata: partition@20000 {
			label = "caldata";
			reg = <0x20000 0x40000>;
			read-only;
		};

		partition@60000 {
			label = "caldata-backup";
			reg = <0x60000 0x40000>;
			read-only;
		};

		partition@a0000 {
			label = "config";
			reg = <0xa0000 0x80000>;
			read-only;
		};

		partition@120000 {
			label = "pot";
			reg = <0x120000 0x80000>;
			read-only;
		};

		partition@1a0000 {
			label = "kernel";
			reg = <0x01a0000 0x0400000>;
		};

		partition@1e0000 {
			label = "ubi";
			reg = <0x05a0000 0x7560000>;
		};

		partition@7b00000 {
			label = "language";
			reg = <0x7b00000 0x200000>;
			read-only;
		};

		partition@7d00000 {
			label = "traffic_meter";
			reg = <0x7d00000 0x300000>;
			read-only;
		};
	};
};

&ref {
	clock-frequency = <40000000>;
};

&eth0 {
	status = "okay";

	phy-handle = <&swphy0>;

	nvmem-cells = <&macaddr_0>;
	nvmem-cell-names = "mac-address";

	gmac-config {
		device = <&gmac>;
		switch-phy-swap = <1>;
	};
};

&eth1 {
	status = "okay";

	nvmem-cells = <&macaddr_6>;
	nvmem-cell-names = "mac-address";
};

&wmac {
	status = "okay";

	mtd-cal-data = <&caldata 0x1000>;

	nvmem-cells = <&macaddr_0>;
	nvmem-cell-names = "mac-address";
};

&pcie {
	status = "okay";

	wifi@0,0,0 {
		compatible = "qcom,ath10k";
		reg = <0 0 0 0 0>;
		nvmem-cells = <&macaddr_c>;
		nvmem-cell-names = "mac-address";
	};
};

&usb {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	hub_port1: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&caldata {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_0: macaddr@0 {
		reg = <0x0 0x6>;
	};

	macaddr_6: macaddr@6 {
		reg = <0x6 0x6>;
	};

	macaddr_c: macaddr@c {
		reg = <0xc 0x6>;
	};
};
