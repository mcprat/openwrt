--- a/drivers/mtd/spi-nor/spi-nor.c
+++ b/drivers/mtd/spi-nor/spi-nor.c
@@ -4385,6 +4385,40 @@ static int spi_nor_setup(struct spi_nor
 
 static void macronix_set_default_init(struct spi_nor *nor)
 {
+	dev_err(nor->dev, "READING STATUS REGISTER");
+
+	write_enable(nor);
+
+	int sr = read_sr(nor);
+	if (sr < 0)
+		return sr;
+
+	if (sr & SR_WIP)
+		dev_err(nor->dev, "STATUS REGISTER BIT 0");
+
+	if (sr & SR_WEL)
+		dev_err(nor->dev, "STATUS REGISTER BIT 1");
+
+	if (sr & SR_BP0)
+		dev_err(nor->dev, "STATUS REGISTER BIT 2");
+
+	if (sr & SR_BP1)
+		dev_err(nor->dev, "STATUS REGISTER BIT 3");
+
+	if (sr & SR_BP2)
+		dev_err(nor->dev, "STATUS REGISTER BIT 4");
+
+	if (sr & SR_TB)
+		dev_err(nor->dev, "STATUS REGISTER BIT 5");
+
+	if (sr & SR_QUAD_EN_MX)
+		dev_err(nor->dev, "STATUS REGISTER BIT 6");
+
+	if (sr & SR_SRWD)
+		dev_err(nor->dev, "STATUS REGISTER BIT 7");
+
+	write_disable(nor);
+
 	nor->params.quad_enable = macronix_quad_enable;
 	nor->params.set_4byte = macronix_set_4byte;
 }
